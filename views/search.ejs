<h1>Browse</h1>
<div>

<%
console.log('urls', urls);
var c = data.length;
while(c--) {
		var emails = data[c];
		var l = emails.length;
		while(l--){
			%>
					<div class="email">
			<a href="<%=urls.get('EMAIL', { id: data[c][l].id, company: data[c][l].company })%>">
			<h4><%=data[c][l].subject%></h4>
			<h5><%=data[c][l].toEmail%></h5>
			</a>

			<%=data[c][l].textBody%>
			</div>

		<%
		}
%><hr />	<%
}
%>

</div>