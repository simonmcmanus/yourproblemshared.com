<h1>Browse</h1>
<div>

<%
console.log('urls', urls);
var c = data.length;
while(c--) {
	%>
		<div class="email">
		<a href="<%=urls.get('EMAIL', { id: data[c][0].id, company: data[c][0].company })%>">
		<h4><%=data[c][0].subject%></h4>
		<h5>to:<%=data[c][0].to%></h5>
		</a>

		<%=data[c][0].textBody
		%>
		</div>
	<%
}
%>

</div>